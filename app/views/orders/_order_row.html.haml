- @orders.each do |order|
  %tr.order-row{"data-href": order_path(order.id)}
    %td
      %span.general-order-number= order.number
    %td
      %span.in-grey-900.font-16.fw-300= order.date_complete
    %td
      %span.font-16.in-grey-900.fw-300= order.order_status
    %td
      %strong.font-16= order.price_with_delivery
    